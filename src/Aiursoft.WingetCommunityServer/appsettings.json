{
  // Database.
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;Cache=Shared"
  },

  "Storage": {
    "Path": "/tmp/data"
  },
  
  "Server": {
    "AgreementUrl": "https://aka.ms/microsoft-store-terms-of-transaction",
    
    "UpstreamGit": "https://git.aiursoft.cn/PublicVault/winget-pkgs.git",
    "UpstreamGitBranch": "master",
    "ShouldUpstreamOnStartup": true
  },

  // Logging.
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}